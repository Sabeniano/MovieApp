@model Movies

<div class="col-sm-4 col-lg-4 col-md-4">
    <div class="thumbnail">
        <img src="@Model.ImageUrl" alt="">
        <div class="caption">
            <h3 class="pull-right">@Model.RunTime</h3>
            <h3>
                <a>@Model.Title</a>
                @*<a>@Model.MovieGenre.Genres.Genre</a>*@
            </h3>
            <p>@Model.Description</p>
        </div>
    </div>
</div>